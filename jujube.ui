    border-top: 0px;
    border-bottom: 0px;
}

QSplitter::handle:hover {
    background-color: lightgray; /* 鼠标悬停在手柄上时的颜色 */
}

QSplitter::handle:pressed {
    background-color: darkgray; /* 按下手柄时的颜色 */
}
